{
  "clangd.enabled": true,
  "clangd.arguments": [
    "--all-scopes-completion",
    "--background-index",
    "--clang-tidy",
    "--completion-style=detailed",
    "--function-arg-placeholders",
    "--header-insertion=iwyu",
    "--limit-results=50",
    "--pch-storage=memory",
    "--query-driver=/usr/local/opt/llvm/bin/clang-*",
    "--suggest-missing-includes",
    "-j=4"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "c",
    "cpp",
    "go",
    "h",
    "hpp",
    "java",
    "json",
    "kt",
    "py",
    "scala",
    "sh"
  ],
  "coc.preferences.formatOnTypeFiletypes": [
    "c",
    "cpp",
    "h",
    "hpp",
    "java",
    "json",
    "kt",
    "py",
    "scala",
    "sh"
  ],
  "coc.preferences.colorSupport": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.previewAutoClose": true,
  "coc.preferences.promptWorkspaceEdit": false,
  "coc.source.buffer.ignoreGitignore": false,
  "coc.source.file.enable": true,
  "codeLens.enable": true,
  "codeLens.subseparator": ";",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.displayByAle": false,
  "diagnostic.enable": true,
  "diagnostic.enableSign": true,
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.messageTarget": "float",
  "diagnostic.errorSign": "E",
  "diagnostic.warningSign": "W",
  "diagnostic.hintSign": "H",
  "diagnostic.infoSign": "I",
  "diagnostic.level": "hint",
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": "> ",
  "html.filetypes": ["html", "handlebars", "htmldjango", "vue"],
  "html.format.enable": true,
  "go.enable": true,
  "go.disable": {
    "completion": false,
    "diagnostics": false,
    "workspaceFolders": false
  },
  "go.goplsUseDaemon": true,
  "go.goplsOptions": {
    "analyses": {
      "asmdecl": true,
      "assign": true,
      "atomic": true,
      "atomicalign": true,
      "bools": true,
      "buildtag": true,
      "cgocall": true,
      "composites": true,
      "copylocks": true,
      "deepequalerrors": true,
      "errorsas": true,
      "fieldalignment": false,
      "fillreturns": true,
      "fillstruct": true,
      "httpresponse": true,
      "ifaceassert": true,
      "loopclosure": true,
      "lostcancel": true,
      "nilfunc": true,
      "nonewvars": true,
      "noresultvalues": true,
      "printf": true,
      "shadow": false,
      "shift": true,
      "simplifycompositelit": true,
      "simplifyrange": true,
      "simplifyslice": true,
      "sortslice": true,
      "stdmethods": true,
      "stringintconv": true,
      "structtag": true,
      "testinggoroutine": true,
      "tests": false,
      "undeclaredname": true,
      "unmarshal": true,
      "unreachable": true,
      "unsafeptr": true,
      "unusedparams": true,
      "unusedresult": true
    },
    "annotations": {
      "bounds": true,
      "escape": true,
      "inline": true,
      "nil": true
    },
    "directoryFilters": ["-ops"],
    "buildFlags": [
        "-tags='wireinject'"
    ],
    "codelenses": {
      "gc_details": false,
      "generate": true,
      "regenerate_cgo": true,
      "tidy": true,
      "upgrade_dependency": true,
      "vendor": false
