    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    },
    "efm": {
      "command": "efm-langserver",
      "args": ["-c", "/Users/aleciverson/.config/efm-langserver/config.yaml"],
      "filetypes": ["vim", "markdown"]
    },
    "haskell": {
      "command": "hie-wrapper",
      "args": ["--lsp"],
