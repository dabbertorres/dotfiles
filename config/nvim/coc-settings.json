    "completeUnimported": true,
    "completionDocumentation": true,
    "env": {},
    "expandWorkspaceToModule": true,
    "gofumpt": true,
    "hoverKind": "FullDocumentation",
    "importShortcut": "Link",
    "linksInHover": true,
    "linkTarget": "pkg.go.dev",
    "staticcheck": true,
    "usePlaceholders": false
  },
  "go.goplsPath": "/Users/aleciverson/Code/Go/bin/gopls",
